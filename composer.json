{
    "name": "daveismyname/laravel-microsoft-graph",
    "description": "A Laravel Microsoft Graph API package",
    "license": "MIT",
    "authors": [
        {
            "name": "David Carr",
            "email": "dave@daveismyname.com",
            "homepage": "https://daveismyname.blog"
        }
    ],
    "homepage": "https://github.com/daveismyname/laravel-microsoft-graph",
    "keywords": ["Laravel", "MsGraph", "Graph", "Microsoft Graph"],
    "require": {
        "illuminate/support": "~5",
        "league/oauth2-client": "^1.4",
        "guzzlehttp/guzzle": "^6"
    },
    "autoload": {
        "psr-4": {
            "Daveismyname\\MsGraph\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daveismyname\\MsGraph\\MsGraphServiceProvider"
            ],
            "aliases": {
                "MsGraph": "Daveismyname\\MsGraph\\Facades\\MsGraph",
                "MsGraphAdmin": "Daveismyname\\MsGraph\\Facades\\MsGraphAdmin"
            }
        }
    }
}
